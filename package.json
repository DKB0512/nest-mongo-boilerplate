{
  "name": "boilerplate-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:create": "hygen seeds create",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest start --watch",
    "start:prod2": "nest build && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.714.0",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/swagger": "8.1.0",
    "@nestjs/typeorm": "10.0.2",
    "@nestjs/websockets": "^10.4.15",
    "@types/multer-s3": "3.0.3",
    "apple-signin-auth": "1.7.8",
    "bcryptjs": "2.4.3",
    "cache-manager": "^6.3.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fb": "2.0.0",
    "google-auth-library": "9.15.0",
    "handlebars": "4.7.8",
    "mongoose": "^8.9.1",
    "ms": "2.1.3",
    "multer": "1.4.5-lts.1",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.5.0",
    "nodemailer": "6.9.16",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "pg": "8.13.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "5.0.1",
    "twitter": "1.7.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.15",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "5.0.0",
    "@types/facebook-js-sdk": "3.3.12",
    "@types/jest": "29.5.14",
    "@types/ms": "0.7.34",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.2",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "@types/twitter": "1.7.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "env-cmd": "10.1.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.7",
    "hygen": "6.2.11",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typescript": "5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "trustedDependencies": [
    "@nestjs/core",
    "@scarf/scarf",
    "@swc/core",
    "core-js"
  ]
}
